<style>
    .div_input_name {
        height:80px;
    }
</style>
<div class="container">
    <form enctype="multipart/form-data" action="/home/edit_store_complete" accept-charset="UTF-8" method="POST">
        <input type="hidden" name="id" value="<%=@store.id%>">
    <div class="div_input_name"> 
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
        <span class="col-xs-4">
            <p>상호명</p>
        </span>
        <input type="text" name="s_name" class="col-xs-8" value="<%=@store.s_name%>">
    </div> 
    <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>전화번호</p>
        </span>
        <input type="text" name="s_number" class="col-xs-8" value="<%=@store.s_number%>">
    </div> 
    <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>배달시간</p>
        </span>
        <input type="text" name="s_deliverytime" class="col-xs-8" value="<%=@store.s_deliverytime%>">
    </div> 
    <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>운영시간</p>
        </span>
        <input type="text" name="s_time" class="col-xs-8" value="<%=@store.s_time%>">
    </div>
    <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>카드가능</p>
        </span>
        <input type="text" name="s_card" class="col-xs-8" value="<%=@store.s_card%>">
    </div>
    <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>배달최소결제금액</p>
        </span>
        <input type="text" name="s_min" class="col-xs-8" value="<%=@store.s_min%>">
    </div>
    <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>주소</p>
        </span>
        <input type="text" name="s_location" class="col-xs-8" value="<%=@store.s_location%>">
    </div>
    <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>음식카테고리</p>
        </span>
        <input type="text" name="s_type" class="col-xs-8" value="<%=@store.s_type%>">
    </div>
     <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>한줄소개</p>
        </span>
        <input type="text" name="s_intro" class="col-xs-8" value="<%=@store.s_intro%>">
    </div>
    <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>배달가능여부</p>
        </span>
        <input type="text" name="s_deliver_ok" class="col-xs-8" value="<%=@store.s_deliver_ok%>">
    </div>
    <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>카드가능여부</p>
        </span>
        <input type="text" name="s_card_ok" class="col-xs-8" value="<%=@store.s_card_ok%>">
    </div>
    <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>분할결제가능여부</p>
        </span>
        <input type="text" name="s_divide_ok" class="col-xs-8" value="<%=@store.s_divide_ok%>">
    </div>
    <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>사진</p>
        </span>
        <input type="file" name="s_picture" class="col-xs-8" value="<%=@store.s_pic%>">
    </div>
    <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>메뉴사진1</p>
        </span>
        <input type="file" name="s_menu_picture" class="col-xs-8" value="<%=@store.s_menu_pic%>">
    </div>
     <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>메뉴사진2</p>
        </span>
        <input type="file" name="s_menu_picture2" class="col-xs-8" value="<%=@store.s_menu_pic%>">
    </div>
     <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>메뉴사진3</p>
        </span>
        <input type="file" name="s_menu_picture3" class="col-xs-8" value="<%=@store.s_menu_pic%>">
    </div>
     <div class="div_input_name"> 
        <span class="col-xs-4">
            <p>메뉴사진4</p>
        </span>
        <input type="file" name="s_menu_picture4" class="col-xs-8" value="<%=@store.s_menu_pic%>">
    </div>
    <button type="submit" class="btn">수정하기</button>
    </form>
</div>